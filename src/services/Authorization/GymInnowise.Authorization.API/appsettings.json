{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ]
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "GymGymVeryBestGym12345SecretKeyNoSwearingbtw",
    "Issuer": "GymIssuer",
    "Audience": "GymAudience",
    "ExpiryInMinutes": 60,
    "RefreshExpiryInMinutes": 10080
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=5432;Database=GymInnowiseAuthorization;Username=postgres;Password=MADL"
  }
}
